    <script src="~/Scripts/jquery-3.4.1.min.js" type="text/javascript"></script>
    <script src="~/Content/plugins/jquery/jquery.min.js" type="text/javascript"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Content/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>

    <style>

        body {
            font-weight: 500;
            font-size: small;
        }

        .btn-primary {
            background: darkcyan !important;
            border-color: darkcyan !important;
        }

        .card-primary:not(.card-outline) > .card-header {
            background: darkcyan !important;
        }

        .icheck-primary > input:first-child:checked + input[type=hidden] + label::before, .icheck-primary > input:first-child:checked + label::before {
            background: darkcyan !important;
            border-color: darkcyan !important;
        }

        .btn-outline-info {
            color: gray !important;
            border-color: gray !important;
        }

            .btn-outline-info:hover {
                color: white !important;
                background-color: gray !important;
                border-color: gray !important;
            }

        textarea.form-control {
            height: 32px;
        }

        .ui-menu {
            background: white !important;
            border: 1px solid !important;
            width: 30% !important;
        }

        .bg-cyan {
            background: darkcyan !important;
        }

        .elipses {
            display: inline-block !important;
            width: 150px !important;
            white-space: nowrap !important;
            overflow: hidden !important;
            text-overflow: ellipsis !important;
        }
        .nav-link > p {
            MARGIN-LEFT: 10PX !IMPORTANT;
        }
    </style>
</head>
<body class="hold-transition sidebar-mini sidebar-collapse">
    <!-- Site wrapper -->
    <div class="wrapper">
        <!-- Preloader -->
        <div class="preloader flex-column justify-content-center align-items-center" style="background:none!important">
            <img src="~/Content/preloader/Blocks-1.3s-237px.gif" alt="CCLogo" height="200" width="200" />

        </div>

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
            </ul>
            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <li class="nav-item d-none d-sm-inline-block">
                    @{
                        if (GYM.Models.Session.AppSession.Session != null && GYM.Models.Session.AppSession.Session.SessionRepo != null)
                        {
                            <div class="row">
                                <a href="#" class="nav-link"><b>
                                    <span class="hidden-xs" style="color: #3b477d;">
                                    ROLE : @GYM.Models.Session.AppSession.Session.SessionRepo[0].role_desc.ToUpper()
                                        </span>&nbsp;&nbsp;&nbsp;</b></a>
                                <a href="#" class="nav-link"><b><span> <span class="hidden-xs" style="color: black;">@GYM.Models.Session.AppSession.Session.SessionRepo[0].usr_full_nme</span></span></b></a>
                            </div>
                        }
                        else
                        {
                            <a href="#" class="nav-link"><b><span> <span class="hidden-xs">No User</span></span></b></a>
                        }
                    }
                </li>
                @*Log Out Button*@
                <li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#" title="Log Out" style="color: black;" onclick="Logout()">
                        <i class="fa fa-sign-out-alt"></i>
                        <span class="badge badge-danger navbar-badge"></span>
                    </a>
                </li>
            </ul>
        </nav>
        <!-- Main Sidebar Container -->

        <aside class="main-sidebar sidebar-dark-cyan elevation-4" style="background: darkcyan;">
            <!-- Brand Logo -->
            <a href="~/Home" class="brand-link">
               
                <img src="~/Content/Logogym.png" alt="GYM Logo" class="brand-image" height="20" width="20" style="position: relative; left: 6px;" />
                <h6 class="brand-text font-weight-normal center-block" style="font-size: small; position:relative; bottom:-3px;"> </h6>
            </a>
            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user (optional) -->
                <!-- SidebarSearch Form -->
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" id="sidebarcustom" data-accordion="false"></ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1>@*Collapsed Sidebar*@</h1>
                        </div>
                        <div class="col-sm-6">
                            @*<ol class="breadcrumb float-sm-right">
                                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                                    <li class="breadcrumb-item"><a href="#">Layout</a></li>
                                    <li class="breadcrumb-item active">Collapsed Sidebar</li>
                                </ol>*@
                        </div>
                    </div>
                </div><!-- /.container-fluid -->
            </section>
            <!-- Main content -->
            <section class="content">
                <div class="container" id="message-alert">
                    <div class="alert alert-success" id="messagetype">
                        <strong id="messageheading">Success!</strong>
                        <span id="message"></span>
                    </div>
                </div>
                <div class="modal fade" id="modalMessages">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header bg-cyan">
                                <h4 class="modal-title">Notification</h4>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick="UnloadMessage()">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <p id="para_message"></p>
                            </div>
                            <div class="modal-footer justify-content-between">
                                <button type="button" class="btn btn-primary" data-dismiss="modal" onclick="UnloadMessage()">Close</button>
                            </div>
                        </div>
                        <!-- /.modal-content -->
                    </div>
                    <!-- /.modal-dialog -->
                </div>
                @RenderBody()

            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="float-right d-none d-sm-block">
                <b>Version</b> 3.1.0
            </div>
            <strong>Copyright &copy; @DateTime.Now.Year <a href="https://ib.jsbl.com/">JS BANK</a>.</strong> All rights reserved.
        </footer>
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">

            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>


    <script src="~/Scripts/Layout.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- Bootstrap 4 -->
    <script src="~/Content/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- Select2 -->
    <script src="~/Content/plugins/select2/js/select2.full.min.js"></script>
    <script src="~/Content/plugins/select2/js/select2.min.js"></script>
    <!-- ChartJS -->
    <script src="~/Content/plugins/chart.js/Chart.min.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Content/plugins/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/Content/plugins/moment/moment.min.js"></script>
    <script src="~/Content/plugins/inputmask/jquery.inputmask.min.js"></script>
    <script src="~/Content/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/Content/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="~/Content/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/Content/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/dist/js/adminlte.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/dist/js/demo.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="~/Content/dist/js/pages/dashboard.js"></script>
    <!-- DataTables  & Plugins -->
    <script src="~/Content/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Content/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Content/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Content/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <script src="~/Content/plugins/datatables-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/Content/plugins/datatables-buttons/js/buttons.bootstrap4.min.js"></script>
    <script src="~/Content/plugins/jszip/jszip.min.js"></script>
    <script src="~/Content/plugins/pdfmake/pdfmake.min.js"></script>
    <script src="~/Content/plugins/pdfmake/vfs_fonts.js"></script>
    <script src="~/Content/plugins/datatables-buttons/js/buttons.html5.min.js"></script>
    <script src="~/Content/plugins/datatables-buttons/js/buttons.print.min.js"></script>
    <script src="~/Content/plugins/datatables-buttons/js/buttons.colVis.min.js"></script>
    <!-- SweetAlert2 -->
    <script src="~/Content/plugins/sweetalert2/sweetalert2.min.js"></script>
    <script src="~/Scripts/SidebarCustom.js"></script>
